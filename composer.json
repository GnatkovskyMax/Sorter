{
  "name": "MaxStepit/Sorter",
  "description": "It`s PHP library in order to sorting array.",
  "keywords": ["project", "library"],
  "type": "library",
  "license": "proprearity",
  "authors": [
    {
      "name": "Maxim Gnatkovsky",
      "email": "Gnatkovsky@bigmir.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "Gnatkovsky\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
